{
    "name": "rishadblack/wire-reports",
    "description": "Create responsive, real-time report views in Laravel using WireReports. Powered by Livewire, this package allows effortless exporting to PDF, Excel, or print, streamlining your reporting process.",
    "license": "AGPL-3.0",
    "authors": [
        {
            "name": "S M Rishad",
            "email": "rishad.black@gmail.com",
            "homepage": "https://smrishad.com"
        }
    ],
    "homepage": "https://github.com/rishadblack/wire-reports",
    "keywords": [
        "Laravel",
        "WireReports"
    ],
    "version": "1.0.7",
    "require": {
        "php": "^8.1",
        "illuminate/support": "^9.0|^10.0|^11.0|^12.0",
        "maatwebsite/excel": "^3.1",
        "barryvdh/laravel-snappy": "^1.0",
        "carlos-meneses/laravel-mpdf": "^2.1",
        "h4cc/wkhtmltopdf-amd64": "^0.12.4"
    },
    "require-dev": {
        "phpunit/phpunit": "~9.0",
        "orchestra/testbench": "^7.0|^8.0|^9.0"
    },
    "autoload": {
        "psr-4": {
            "Rishadblack\\WireReports\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Rishadblack\\WireReports\\Tests\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Rishadblack\\WireReports\\WireReportsServiceProvider"
            ],
            "aliases": {
                "WireReports": "Rishadblack\\WireReports\\Facades\\WireReports"
            }
        }
    }
}
